{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "rules": {
        "no-console": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "variable-name": false,
        "no-var-requires": false,
        "one-variable-per-declaration": false,
        "no-empty": false,
        "prefer-const": false
    }
}
